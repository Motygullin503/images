        {
          "text": "Male pattern hair loss (hereditary-patterned baldness)",
          "resource-id": "com.healthagen.iTriage:id/text_1",
          "ancestors": [
            "android.view.View",
            "java.lang.Object"
          ],
          "clickable": false,
