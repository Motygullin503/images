      "resource-id": "pl.agora.mojedziecko:id/section_holder_layout",
      "children": [
        {
          "text": "Siatki centylowe",
          "resource-id": "pl.agora.mojedziecko:id/title",
          "ancestors": [
            "android.widget.TextView",
            "android.view.View",
            "java.lang.Object"
          ],
          "clickable": false,
          "class": "android.support.v7.widget.AppCompatTextView",
          "bounds": [
            88,
