        {
          "text": "Gallows & Anderson surge back on Raw",
          "resource-id": "com.wwe.universe:id/news_item_txt_subtitle",
          "ancestors": [
            "android.widget.TextView",
            "android.view.View",
            "java.lang.Object"
          ],
          "clickable": false,
          "class": "com.bottlerocketapps.ui.brfont.BRFontTextView",
