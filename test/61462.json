      "children": [
        {
          "text": "D-87 board OKs work at Sheridan, Raymond",
          "resource-id": "com.particlenews.newsbreak:id/news_title",
          "ancestors": [
            "android.widget.TextView",
            "android.view.View",
            "java.lang.Object"
